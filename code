#include "stdafx.h"
#include "iostream"
#include "math.h"
#include "fstream"
using namespace std;

void main()
{
	int i, k, n=0;
	double D = 0, Dk, Dn;
	ifstream fin("numb.txt");
	for (i = 0; !fin.eof(); i++)
	{
		fin >> k;
		D = D + pow(k, 2);
		n++;
	}
	Dn = D / n;
	if (Dn < 0)
		cout << "Error" << endl;
	else
	{
